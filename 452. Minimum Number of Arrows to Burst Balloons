https://leetcode.com/problems/minimum-number-of-arrows-to-burst-balloons/

class Solution {
    public int findMinArrowShots(int[][] points) {
        Arrays.sort(points, (a,b) -> {
            if (a[0] < b[0]) return -1;
            else return 1;
        });
        int count = 1;
        int prev = points[0][1];
        for (int i = 1; i < points.length; i++) {
            if (points[i][0] > prev) {
                prev = points[i][1];
                count++;
            } else {
                prev = Math.min(prev, points[i][1]);
            }
        }
        return count;
    }
}
